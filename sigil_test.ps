%!
(dmmlib/base.ps) run
(dmmlib/lines.ps) run
(sigils.ps) run

/nib_angle -30 def

/nstroke {
    gsave
        nib_angle rotate
        0.5 1 scale
        0.5 callistroke
    grestore
    newpath
} def

72 softscale
4.25 5.5 translate
3 thicken

sigil_bank /array known {
    sigil_bank /array get exec
} if
